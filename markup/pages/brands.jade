extends main

block head
    - var headData = {title: "О бутике"}
    //- var index = 'header_index'

    link(href="%=static=%css/separate-css/slick.css", rel="stylesheet", type="text/css")

block content
    .container
        .title-table
            .title-table__cell
                h1.ttu.h1 Бренды
            .title-table__cell.text-right.hidden-sm-down
                .form-search(style="width: 270px;")
                    input.form-search__input(placeholder="Поиск по каталогу")
                    button.form-search__btn: +icon('search', 'svg-search', 15, 15)

        br
        br.hidden-sm-down
        .brand
            .brand__item
                .brand__item-img: +img('content/brand-catalog/1')
                .brand__item-desc.text-center
                    .brand__item-desc-img: +img('content/brand-catalog/brand-1')
                    p.brand__item-desc-text TWINSET Simona Barbieri – это многопрофильная компания с ярко выраженной идентичностью и страстью к инновациям. Движущей силой бренда всегда были креативность, интуиция, предпринимательский дух и энтузиазм.
                    .brand__item-btnList.not-styleLink
