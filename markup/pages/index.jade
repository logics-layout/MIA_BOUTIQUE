        br
        br
        br
    .clothing.not-styleLink
        a(href="#").clothing__section
            .clothing__item
                .clothing__item-cell
                    .clothing__item-img: +img('content/clothing/1', 'png')
                    .clothing__item-title Платья
        a(href="#").clothing__section
            .clothing__item
                .clothing__item-cell
                    .clothing__item-img: +img('content/clothing/2', 'png')
